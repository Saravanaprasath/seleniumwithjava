<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Get titles of youtube">
    <test name="Go to youtube">
        <classes>
            <class name="auto.inside.automation.applications.youtube.tests.HomePageTests">
                <methods>
                    <include name="searchInGoogle"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Print all results">
        <classes>
            <class name="auto.inside.automation.applications.youtube.tests.HomePageTests">
                <methods>
                    <include name="printTitles"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>