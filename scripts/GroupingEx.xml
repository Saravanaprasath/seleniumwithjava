<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Grouping example">
    <test name="Testing Group">
        <groups>
            <run>
                <include name="moto"/>
                <include name="apple"/>
                <exclude name="vivo"/>
                <include name="oneplus"/>
            </run>
        </groups>
        <classes>
            <class name="org.saravana.learntestng.GroupingExample"/>
        </classes>
    </test>
</suite>